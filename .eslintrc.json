{
    "env": {
      "node": true
    },
    "extends": "airbnb",
    "rules": {
      "keyword-spacing": 0,
      "no-plusplus": 0,
      "no-undef": 0,
      "space-in-parens": 0,
      "prefer-destructuring": 0,
      "space-before-blocks": 0,
      "space-infix-ops": 0,
      "quotes": 0,
      "padded-blocks": 0,
      "block-spacing": 0,
      "key-spacing": 0,
      "func-names": 0,
      "space-before-function-paren": 0,
      "object-shorthand": 0,
      "prefer-arrow-callback": 0,
      "class-methods-use-this": 0,
      "brace-style": 0
    }
}
